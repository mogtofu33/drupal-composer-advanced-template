    "replace": {
        "bower-asset/backbone": "*",
        "bower-asset/bootstrap": "*",
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/underscore": "*",
        "npm-asset/backbone": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/underscore": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "require-dev": {
        "bower-asset/d3": "^5.1",
        "bower-asset/highlightjs": "^9.12",
        "drupal/core-dev": "9.0",
        "drupal/devel": "^4.0",
        "ergebnis/composer-normalize": "^2.2",
        "mglaman/phpstan-drupal": "^0.12.6",
        "nymedia/drupal_settings": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.5",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "config": {
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "drupal/core": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bootstrap-theme": {
            "title": "My BS subtheme",
            "name": "my_subtheme"
        },
        "composer-exit-on-patch-failure": false,
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the project from the </>",
                "<bg=blue;fg=white>  mog33/drupal-composer-advanced-template template!      </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the README",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": {
                    "append": "assets/settings.append.txt"
                },
                "[web-root]/sites/example.settings.local.php": {
                    "mode": "replace",
                    "path": "assets/example.settings.local.php",
                    "overwrite": true
                },
                "[web-root]/sites/default/settings.prod.php": {
                    "mode": "replace",
                    "path": "assets/settings.prod.php",
                    "overwrite": false
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": true
                }
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "bower-asset/slick-carousel"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "https://drupal.org/node/3104566 Implement Server Timing performance metrics": "https://www.drupal.org/files/issues/2020-06-24/3104566-58.patch",
                "https://drupal.org/node/2869592 Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ProjectHelper.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.15.1",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.15.1.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@setup-project"
        ],
        "post-update-cmd": [
            "@setup-project"
        ],
        "post-create-project-cmd": [
            "@setup-project"
        ],
        "nuke": "rm -rf composer.lock web/core web/libraries web/*.php web/modules/contrib web/profiles/contrib web/themes/contrib vendor .editorconfig .env .gitattributes",
        "setup-project": [
            "DrupalProject\\composer\\ProjectHelper::createRequiredFiles"
        ]
    }
}
