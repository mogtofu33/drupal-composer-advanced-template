        "drupal/viewsreference": "^2@BETA",
        "drupal/webp": "^1@RC",
        "drush/drush": "^12",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "replace": {
        "bower-asset/backbone": "*",
        "bower-asset/bootstrap": "*",
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/underscore": "*",
        "npm-asset/backbone": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/underscore": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "require-dev": {
        "bower-asset/d3": "~3.5",
        "bower-asset/highlightjs": "~9.16",
        "drupal/core-dev": "^10.2@STABLE",
        "drupal/devel": "^5.2",
        "drupal/devel_generate": "^5.2",
        "drupal/webprofiler": "^10.1",
        "ergebnis/composer-normalize": "^2.42",
        "mglaman/phpstan-drupal": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "config": {
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "drupal/core": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "ergebnis/composer-normalize": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": false,
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the project from the </>",
                "<bg=blue;fg=white>  mog33/drupal-composer-advanced-template template!      </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the README",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": {
                    "append": "assets/settings.append.txt"
                },
                "[web-root]/sites/default/settings.dev.php": {
                    "mode": "replace",
                    "overwrite": false,
                    "path": "assets/settings.dev.php"
                },
                "[web-root]/sites/default/settings.prod.php": {
                    "mode": "replace",
                    "overwrite": false,
                    "path": "assets/settings.prod.php"
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "overwrite": true,
                    "path": "assets/development.services.yml"
                },
                "[web-root]/sites/example.settings.local.php": {
                    "mode": "replace",
                    "overwrite": true,
                    "path": "assets/example.settings.local.php"
                }
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "bower-asset/slick-carousel"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Fix Claro styles for exposed views filters wrapped in fieldsets": "https://www.drupal.org/files/issues/2021-05-31/3133639.19.patch",
                "Let users configure the text of the \"Add media\" button": "https://www.drupal.org/files/issues/2021-10-01/3169956-34.patch"
            },
            "drupal/focal_point": {
                "Preview link accidentally closes the media library": "https://www.drupal.org/files/issues/2020-10-06/3162210-17.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ProjectHelper.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@setup-project"
        ],
        "post-update-cmd": [
            "@setup-project"
        ],
        "post-create-project-cmd": [
            "@setup-project"
        ],
        "nuke": "rm -rf composer.lock web/core web/libraries web/*.php web/modules/contrib web/profiles/contrib web/themes/contrib vendor .editorconfig .env .gitattributes",
        "setup-project": [
            "DrupalProject\\composer\\ProjectHelper::createRequiredFiles"
        ]
    },
    "support": {
        "issues": "https://gitlab.com/mog33/drupal-composer-advanced-template/-/issues",
        "source": "https://gitlab.com/mog33/drupal-composer-advanced-template"
    }
}
